{
  "name": "turtle-quest",
  "version": "0.6.2",
  "homepage": "https://danieldesira.github.io/TurtleQuest",
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "gh-pages": "^6.2.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.25.13",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "dev": "npx vite",
    "start": "npm run dev",
    "docs": "npx typedoc src/characters/abstract/* src/characters/interfaces/* src/levels/* src/utils/* src/enums/* src/Background.ts src/checkTurtle.ts src/Game.ts --name TurtleQuest --out dev/docs/",
    "build": "tsc -b && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "^2.1.0",
    "@vercel/analytics": "^1.2.2",
    "@vitejs/plugin-react": "^4.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "vite": "^5.4.10"
  }
}
